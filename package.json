{
  "name": "@govconnex/ui",
  "version": "0.0.167",
  "description": "GovConnex UI - React Component Library",
  "scripts": {
    "build:tokens": "./tokens-build.sh",
    "prepublishOnly": "rm -rf dist && rollup -c",
    "test": "jest --testPathIgnorePatterns \"src/puppeteer-tests/.*\" --env=jsdom",
    "test:storyshots": "yarn build && jest src/puppeteer-tests/storyshots-puppeteer.test.js --env=node --config=jest.config.js --runInBand --detectOpenHandles",
    "prestart": "yarn run build:tokens",
    "start": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "build": "NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "watch": "NODE_ENV=development yarn run prepublishOnly --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "formatCheck": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint": "eslint --max-warnings=0 .",
    "prepare": "yarn run build:tokens",
    "new": "plop",
    "link:all": "yarn link:react && yarn link:react-dom && yarn link:styled-components && yarn link",
    "link:react": "cd node_modules/react && yarn link && cd ../../",
    "link:react-dom": "cd node_modules/react-dom && yarn link && cd ../../",
    "link:styled-components": "cd node_modules/styled-components && yarn link && cd ../../"
  },
  "author": "GovConnex",
  "license": "ISC",
